<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>NepalGov360</title>
        <link rel="stylesheet" href="../styles.css" />

        <!-- <link href="/css/styles.css" rel="stylesheet"> -->
    </head>
    <body>
        <%- include('partials/header') %>

        <div class="container mx-auto p-4">
            <h1 class="text-2xl font-bold text-green-700">Government Data</h1>
            <div id="data-list">
                <% data.forEach(item => { %>
                <div>
                    <h1><%= item.title %></h1>
                    <p><%= item.number %></p>
                </div>

                <% }) %>
            </div>
        </div>

        <%- include('partials/footer') %>
    </body>
</html>
